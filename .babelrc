{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "lodash",
  ],
  "presets": [
    "@babel/preset-flow",
    [
      "@babel/preset-env", {
        "targets": {
          // solve problems with async/await, see: https://github.com/babel/babel/issues/5085#issuecomment-363242788
          "node": "current",
        },
      },
    ],
  ],
}
