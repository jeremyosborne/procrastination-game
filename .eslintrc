{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "plugins": [
     "import",
  ],
  "settings": {
    "import/resolver": "webpack",
  },
  "rules": {
    "import/no-unresolved": ["error"],
    "comma-dangle": ["error", "only-multiline"],
    "object-curly-spacing": ["error", "never"],
    "yoda": ["off"],
  },
}
